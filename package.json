{
  "name": "@hinzky/atropos-vue",
  "version": "1.0.0",
  "description": "Vue 3 wrapper around Atropos",
  "keywords": [
    "atropos",
    "vue",
    "vue3",
    "card",
    "3d",
    "parallax",
    "component",
    "hover",
    "javascript",
    "portfolio",
    "gallery",
    "showcase"
  ],
  "homepage": "https://github.com/enrique-orellana/atropos-vue#readme",
  "bugs": {
    "url": "https://github.com/enrique-orellana/atropos-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enrique-orellana/atropos-vue.git"
  },
  "license": "ISC",
  "author": "Enrique Orellana",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/node": "^24.10.1",
    "atropos": "^2.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "rimraf": "^6.1.0",
    "typescript": "5.3.3",
    "vite": "^7.2.2",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  }
}
